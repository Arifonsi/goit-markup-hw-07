.backdrop {
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  position: fixed;
  background-color: rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
  justify-content: center;
  display: flex;
  transition: opacity 250ms linear, visibility 250ms linear;
}
.backdrop.backdrop.is-hidden {
  opacity: 0;
  visibility: hidden;
  pointer-events: none;
}

.modal {
  position: relative;
  width: 528px;
  height: 581px;
  background: rgb(255, 255, 255);
  border: 1px solid black;
  border-radius: 4px;
  margin: auto;
  box-shadow: 0px 2px 1px 0px rgba(0, 0, 0, 0.2);
  box-shadow: 0px 1px 1px 0px rgba(0, 0, 0, 0.14);
  box-shadow: 0px 1px 3px 0px rgba(0, 0, 0, 0.12);
}
.modal__close__btn {
  position: absolute;
  right: 16px;
  top: 16px;
  border-radius: 18px;
  border: 1px solid rgba(0, 0, 0, 0.1);
  background-color: transparent;
}
.modal__close__btn__icon {
  width: 30px;
  height: 30px;
  border: 1 px solid black;
  cursor: pointer;
  padding: 0;
}
.modal__close__btn:hover, .modal__close__bn:focus {
  fill: black;
  border-color: #2196f3;
}

